<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:weightSum="15"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.main.DetailMatch">

        <FrameLayout
            android:id="@+id/id1"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="#111110">

            <Button
                android:id="@+id/iddtmhome"
                android:layout_width="17dp"
                android:layout_height="15dp"
                android:background="@drawable/back">

            </Button>

            <ImageView
                android:id="@+id/idTeam1dm"
                android:layout_width="58dp"
                android:layout_height="50dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="16dp"
                android:scaleType="fitXY">

            </ImageView>


            <ImageView
                android:id="@+id/idTeam2dm"
                android:layout_width="52dp"
                android:layout_height="51dp"
                android:layout_gravity="right"
                android:layout_marginTop="16dp"
                android:layout_marginRight="40dp"
                android:scaleType="fitXY">

            </ImageView>

            <TextView
                android:id="@+id/idStagedm"
                android:layout_width="167dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:textColor="#F7F2F2"
                android:textSize="12sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/idScore1dm"
                android:layout_width="28dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:layout_marginRight="80dp"
                android:layout_marginBottom="15dp"
                android:gravity="center"
                android:textColor="#F7F2F2"
                android:textSize="16sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/idScore2dm"
                android:layout_width="31dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="83dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="15dp"
                android:gravity="center"
                android:textColor="#F7F2F2"
                android:textSize="16sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="25dp"
                android:layout_height="23dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:layout_marginBottom="11dp"
                android:background="@drawable/logovs">

            </TextView>
            <TextView
                android:id="@+id/idrounddm"
                android:layout_width="87dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="45dp"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:textColor="#F7F2F2"
                android:textSize="12sp"
                android:textStyle="bold">

            </TextView>
            <TextView
                android:id="@+id/idFulltimedtm"
                android:layout_width="87dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="49dp"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:textColor="#F7F2F2"
                android:textSize="12sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/idLeagueDetailM"
                android:layout_width="149dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center"
                android:layout_marginLeft="3dp"
                android:layout_marginBottom="2dp"
                android:gravity="center"
                android:textColor="#F1F1EC"
                android:textSize="12sp">
            </TextView>

            <TextView
                android:id="@+id/idName1dm"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginLeft="7dp"
                android:layout_marginBottom="2dp"
                android:gravity="center_horizontal"
                android:paddingLeft="5dp"
                android:textColor="#F7F2F2"
                android:textSize="12sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/idName2dm"
                android:layout_width="113dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|right"
                android:layout_marginRight="7dp"
                android:layout_marginBottom="2dp"
                android:gravity="center_horizontal"
                android:paddingRight="5dp"
                android:textColor="#F7F2F2"
                android:textSize="12sp"
                android:textStyle="bold">
            </TextView>
        </FrameLayout>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/idFramelayoutdtm"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/idbottomdtm"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="?android:attr/windowBackground"
                app:backgroundTint="#0C0C0C"
                app:itemBackground="@color/bottomDetailmatch"
                app:itemIconTint="#DF405E"
                android:layout_gravity="top"
                app:itemRippleColor="@color/colorRed"
                app:itemTextColor="#31AC33"
                android:foreground="?attr/selectableItemBackground"
                app:labelVisibilityMode="labeled"
                app:menu="@menu/bottom_navigationdm">
            </com.google.android.material.bottomnavigation.BottomNavigationView>
        </FrameLayout>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</LinearLayout>